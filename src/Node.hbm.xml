<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="edu.kit.ipd.sonar.server.Node" table="Node" lazy="false">
        <id name="id" column="nodeId" type="integer">
            <generator class="native" />
        </id>
        
        <property name="name" column="nodeName" type="string" insert="false" update="false" />
        <property name="originalWeight" column="weight" type="double" insert="false" update="false" />
        <property name="createTime" column="createTime" type="integer" insert="false" update="false" />
        
    </class>
</hibernate-mapping>