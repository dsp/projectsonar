<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="edu.kit.ipd.sonar.server.User" table="Userlist" lazy="false">
        <id name="userid" column="id" type="integer">
            <generator class="native" />
        </id>
        
        <property name="name" column="username" type="string" insert="false" update="false" />
        <!--  <property name="password" column="password" type="string" insert="false" update="false" /> -->

    </class>
</hibernate-mapping>
